class Solution {
public:
    string reverseString(string s) {
        string sr; 
        stack<char> ss; 
        for(int i=0; i< s.size(); i++)
        {   
                ss.push(s[i]);
        }    
        while(!ss.empty())
        {   
                char cc= ss.top();
                sr+= cc; 
                ss.pop();
        }   
        return sr; 
    }   
};
